<?xml version="1.0"?>
<robot name="tello">

    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>
    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>
    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>
    <material name="grey">
        <color rgba="0.75 0.75 0.75 1"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

	<!-- * * * Link Definitions * * * -->
    <link name="base_link">
    </link>

    <link name="body_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.052" />
            <inertia ixx="0.000012118125" ixy="0.0" ixz="0.0" iyy="0.0000308618125" iyz="0.0" izz="0.000034625"/>
        </inertial>  

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_body.dae"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="0.0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_body.dae"/>
            </geometry>
            <material name="white"/>
        </visual>
	</link>

    <link name="arms_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.023" />
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0002835" iyz="0.0" izz="0.000324"/>
        </inertial>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_arms.dae"/>
            </geometry>
        </collision>
  
        <visual>
            <origin rpy="0.0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_arms.dae"/>
            </geometry>
            <material name="black"/>
        </visual>
	</link>
    
    <link name="back_right_propeller_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.003" />
            <inertia ixx="0.0000016762" ixy="0.0" ixz="0.0" iyy="0.0000000823" iyz="0.0" izz="0.00000166468"/>
        </inertial>
        
        <collision>
            <origin xyz="-0.045 -0.05 0.038318" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_back_right_propeller.dae"/>
            </geometry>
        </collision>
  
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_back_right_propeller.dae"/>
            </geometry>
            <material name="grey"/>
        </visual>
	</link>

    <link name="back_left_propeller_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.003" />
            <inertia ixx="0.0000016762" ixy="0.0" ixz="0.0" iyy="0.0000000823" iyz="0.0" izz="0.00000166468"/>
        </inertial>

        <collision>
            <origin xyz="-0.045 0.05 0.038318" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_back_left_propeller.dae"/>
            </geometry>
        </collision>        

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_back_left_propeller.dae"/>
            </geometry>
            <material name="grey"/>
        </visual>
	</link>

    <link name="front_left_propeller_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.003" />
            <inertia ixx="0.0000016762" ixy="0.0" ixz="0.0" iyy="0.0000000823" iyz="0.0" izz="0.00000166468"/>
        </inertial>

        <collision>
            <origin xyz="0.04 0.05 0.038318" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_front_left_propeller.dae"/>
            </geometry>
        </collision>        

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_front_left_propeller.dae"/>
            </geometry>
            <material name="grey"/>
        </visual>
	</link>

    <link name="front_right_propeller_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.003" />
            <inertia ixx="0.0000016762" ixy="0.0" ixz="0.0" iyy="0.0000000823" iyz="0.0" izz="0.00000166468"/>
        </inertial>  

        <collision>
            <origin xyz="0.04 -0.05 0.038318" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_front_right_propeller.dae"/>
            </geometry>
        </collision>        

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://tcc_tello/description/meshes/tello/tello_front_right_propeller.dae"/>
            </geometry>
            <material name="grey"/>
        </visual>
	</link>

	<!-- * * * Joint Definitions * * * -->
    <joint name="body_joint" type="fixed">
        <parent link="base_link"/>
        <child link="body_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <joint name="arms_joint" type="fixed">
        <parent link="base_link"/>
        <child link="arms_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

       <joint name="back_right_propeller_joint" type="continuous">
    	<parent link="arms_link"/>
    	<child link="back_right_propeller_link"/>
    	<origin xyz="-0.045 -0.05 0.038318" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
	</joint>

    <joint name="back_left_propeller_joint" type="continuous">
    	<parent link="arms_link"/>
    	<child link="back_left_propeller_link"/>
    	<origin xyz="-0.045 0.05 0.038318" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
	</joint>

    <joint name="front_left_propeller_joint" type="continuous">
    	<parent link="arms_link"/>
    	<child link="front_left_propeller_link"/>
    	<origin xyz="0.04 0.05 0.038318" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
	</joint>


    <joint name="front_right_propeller_joint" type="continuous">
    	<parent link="arms_link"/>
    	<child link="front_right_propeller_link"/>
    	<origin xyz="0.04 -0.05 0.038318" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
	</joint>


</robot>
